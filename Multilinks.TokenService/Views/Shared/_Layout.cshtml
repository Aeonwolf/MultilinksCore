<!DOCTYPE html>
<html>
<head>
   <meta name="format-detection" content="telephone=no" />
   <meta charset="utf-8" />
   <meta name="viewport" content="width=device-width, initial-scale=1.0" />

   <title>@ViewData["Title"] - Multilinks | Create Connect Communicate</title>

   <link rel="icon" href="../images/favicon.ico" type="image/x-icon">
   <link rel="stylesheet" href="../css/grid.css">
   <link rel="stylesheet" href="../css/camera.css">
   <link rel="stylesheet" href="../css/animate.css">
   <link rel="stylesheet" href="../css/style.css">

   <script src="../js/jquery.js"></script>
   <script src="../js/jquery-migrate-1.2.1.js"></script>
   <script src='../js/device.min.js'></script>
</head>
<body>
   <div class="page">
      <!--========================================================
                                HEADER
      =========================================================-->
      <header>
         <div class="container">
            <div class="header-wr">
               <div class="brand">
                  <h1 class="brand_name">
                     <a asp-area="" asp-controller="Home" asp-action="Index"><span class="brand_name_a">Multi</span><span class="brand_name_b">links</span></a>
                  </h1>
                  <p class="brand_slogan">
                     Create Connect Communicate
                  </p>
               </div>
            </div>
         </div>
         <div id="stuck_container" class="stuck_container">
            <div class="container">
               <nav class="nav">
                  <ul class="sf-menu" data-type="navbar">
                     <li>
                        <a href="#">Register</a>
                     </li>
                     <li>
                        <a href="#">Multilinks Clients</a>
                     </li>
                     <li>
                        <a href="#">Multilinks Gateways</a>
                     </li>
                     <li>
                        <a href="#">Support</a>
                     </li>
                  </ul>
               </nav>
            </div>
         </div>
      </header>

      <!--========================================================
                                CONTENT
      =========================================================-->
      <main>
         @RenderBody()
      </main>

      <!--========================================================
                                FOOTER
      =========================================================-->
      <footer>
         <div class="container">
            <div class="copyright">
               <a asp-area="" asp-controller="Home" asp-action="Index">MDS Multilinks</a> © <span id="copyright-year"></span> | <a href="#">Privacy Policy</a> | <a href="#">Terms &amp; Conditions</a>
            </div>
         </div>
      </footer>
   </div>

   @RenderSection("Scripts", required: false)
   <script src="../js/script.js"></script>
</body>
</html>
